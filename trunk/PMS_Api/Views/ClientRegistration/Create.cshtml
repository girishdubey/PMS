@model PMS_Entity.Model.Master.ClientRegistration

@{
    ViewBag.Title = "Create";
}

<link href="~/bootstrap/css/bootstrap.css" rel="stylesheet" />

@using (Html.BeginForm())
{



    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div class="container">

        <h2>Registration</h2>
        <div class="row">
            <div class="col-md-4">
                @Html.LabelFor(model => model.FirstName)
                @Html.TextBoxFor(model => model.FirstName, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.FirstName)
            </div>

            <div class="col-md-4">
                @Html.LabelFor(model => model.Lastname)
                @Html.TextBoxFor(model => model.Lastname, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.Lastname)
            </div>

            <div class="col-md-4">
                @Html.LabelFor(model => model.Gender)
                @Html.TextBoxFor(model => model.Gender, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.Gender)
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                @Html.LabelFor(model => model.Username)
                @Html.TextBoxFor(model => model.Username, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.Username)
            </div>
            <div class="col-md-4">
                @Html.LabelFor(model => model.Password)
                @Html.TextBoxFor(model => model.Password, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.Password)
            </div>
            <div class="col-md-4">
                @Html.LabelFor(model => model.Mobileno)
                @Html.TextBoxFor(model => model.Mobileno, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.Mobileno)
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                @Html.LabelFor(model => model.EmailID)
                @Html.TextBoxFor(model => model.EmailID, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.EmailID)
            </div>

            <div class="col-md-4">
                @Html.LabelFor(model => model.Token)
                @Html.TextBoxFor(model => model.Token, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.Token)
            </div>

            <div class="col-md-4">
                @Html.LabelFor(model => model.EncryKey)
                @Html.TextBoxFor(model => model.EncryKey, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.EncryKey)
            </div>
        </div>
        <div class="row">
            <div class="col-md-4">
                @Html.LabelFor(model => model.IVKey)
                @Html.TextBoxFor(model => model.IVKey, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.IVKey)
            </div>

            <div class="col-md-4">
                @Html.LabelFor(model => model.UniqueID)
                @Html.TextBoxFor(model => model.UniqueID, new { @class = "form-control", autocomplete = "off" })
                @Html.ValidationMessageFor(model => model.UniqueID)
            </div>

            <div class="col-md-4">
                <br />
                <input type="submit" class="btn btn-success" value="Create" />
            </div>

        </div>
    </div>

}


@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
